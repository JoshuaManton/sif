#include "core:basic.sif"

struct Foo!($Arg: typeid, $Arg: int) {
    proc wow() {
        printa("Foo");
    }
}

struct Bar!($Arg: string) {
    proc wow() {
        printa("Bar");
    }
}

proc work(foo: $T) {
    T.wow();
    printa(T.Arg);
}

proc main() : i32 {
    var foo: Foo!(float);
    var bar: Bar!(123);
    work(foo);
    work(bar);
    return 0;
}
