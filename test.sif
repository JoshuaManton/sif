#include "core:basic.sif"

struct Foo {
    var a: int;
    var b: cstring;
    var c: bool;
}

proc main() : i32 {
    var foo = Foo{1, "wow", true};
    print("%\n", foo);
    foo = #partial {1, "wow"};
    print("%\n", foo);

    var arr: [3]float = #partial {1, 2};
    print("%\n", arr);
    return 0;
}
