struct Foo {
    var asd: int;
}

proc get_foo() : ^Foo {
    return new_clone(Foo{123}, default_allocator());
}

proc main() : i32 {
    if var foo = get_foo(); (foo != null) {
        printa(foo^);
    }
    else {
        printa("no foo");
    }
    return 0;
}
